        <mass>0.25</mass> <!--0.25-->

        <!-- CUBOID INERTIA, x,y,z,mass
          ixx="${mass*(y*y+z*z)/12}" ixy="0.0" ixz="0.0"
          iyy="${mass*(x*x+z*z)/12}" iyz="0.0"
          izz="${mass*(y*y+x*x)/12}"/>
        -->
        
        <!-- x = 0.1 y = 0.08 z = 0.06, mass = 0.25 -->

        <inertia>
          <ixx>0.00020833333</ixx> <!-- 0.00020833333 -->
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.00028333333</iyy> <!-- 0.00028333333 -->
          <iyz>0</iyz>
          <izz>0.00034166666</izz> <!-- 0.00034166666 -->
        </inertia>
      </inertial>
    </link>

